{
   "name": "front-v2",
   "private": true,
   "version": "0.0.1",
   "type": "module",
   "engines": {
      "node": ">= 18.x",
      "npm": ">= 9.x"
   },
   "scripts": {
      "clean": "rm -rf dist node_modules",
      "install:clean": "rm -rf node_modules && yarn",
      "dev": "vite",
      "prebuild": "rm -rf dist",
      "build": "tsc && vite build",
      "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
      "lint:fix": "eslint --fix .",
      "preview": "vite preview",
      "serve": "serve -s dist",
      "test": "vitest",
      "test:run": "vitest run",
      "test:coverage": "vitest --coverage",
      "test:e2e": "cypress open",
      "prettier": "prettier \"*.@(js|ts|tsx|json|md)\"",
      "prettier:check": "yarn prettier --check .",
      "prettier:fix": "yarn prettier --write .",
      "format:check": "yarn prettier:check && yarn lint",
      "format:fix": "yarn prettier:fix && yarn lint:fix",
      "precommit:all": "yarn clean && yarn && yarn prettier:check && yarn lint && vitest run && yarn build",
      "precommit": "yarn prettier:check && yarn lint && yarn test:run && yarn build"
   },
   "dependencies": {
      "@emotion/react": "11.11.3",
      "@emotion/styled": "11.11.0",
      "@hookform/resolvers": "3.3.4",
      "@mui/icons-material": "5.15.6",
      "@mui/lab": "5.0.0-alpha.165",
      "@mui/material": "5.15.2",
      "@mui/x-data-grid": "^7.4.0",
      "@mui/x-tree-view": "^7.3.1",
      "@reduxjs/toolkit": "1.9.7",
      "axios": "1.6.2",
      "dotenv": "16.4.1",
      "file-saver": "2.0.5",
      "i18next": "23.5.1",
      "i18next-browser-languagedetector": "7.1.0",
      "is-valid-domain": "^0.1.6",
      "js-cookie": "3.0.5",
      "jwt-decode": "^4.0.0",
      "keycloak-js": "23.0.1",
      "lodash": "4.17.21",
      "moment": "^2.30.1",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-file-icon": "^1.5.0",
      "react-hook-form": "7.50.1",
      "react-hot-toast": "2.4.1",
      "react-i18next": "13.2.2",
      "react-icons": "4.12.0",
      "react-redux": "8.1.3",
      "react-router-dom": "6.16.0",
      "react-svg": "16.1.29",
      "react-tooltip": "5.21.6",
      "redux": "4.2.1",
      "sass-loader": "13.3.2",
      "serve": "14.2.1",
      "zod": "3.22.4"
   },
   "devDependencies": {
      "@testing-library/jest-dom": "6.1.4",
      "@testing-library/react": "14.0.0",
      "@testing-library/user-event": "14.5.2",
      "@types/file-saver": "2.0.7",
      "@types/js-cookie": "3.0.6",
      "@types/lodash": "4.14.202",
      "@types/react": "18.2.25",
      "@types/react-dom": "18.2.7",
      "@types/react-file-icon": "^1.0.4",
      "@typescript-eslint/eslint-plugin": "6.0.0",
      "@typescript-eslint/parser": "6.0.0",
      "@vitejs/plugin-react-swc": "3.3.2",
      "@vitest/coverage-v8": "0.34.6",
      "cypress": "13.3.1",
      "eslint": "8.45.0",
      "eslint-plugin-react-hooks": "4.6.0",
      "eslint-plugin-react-refresh": "0.4.3",
      "jsdom": "22.1.0",
      "msw": "2.1.6",
      "prettier": "3.0.3",
      "sass": "1.69.2",
      "sass-loader": "13.3.2",
      "typescript": "5.0.2",
      "vite": "4.4.5",
      "vite-tsconfig-paths": "4.2.1",
      "vitest": "1.6.0"
   },
   "peerDependencies": {
      "react": "^17.0.0 || ^18.0.0",
      "react-dom": "^17.0.0 || ^18.0.0"
   }
}
